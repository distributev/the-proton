{
    "name": "theproton",
    "description": "The Proton App",
    "productName": "TheProtonApp",
    "version": "0.0.1",
    "author": "Luis Manuel <luisman40@gmail.com>",
    "license": "MIT",
    "main": "background.js",
    "build": {
        "appId": "com.distributev.theprotonapp",
        "asar": false,
        "files": [
            "**/*"
        ],
        "mac": {
            "target": "zip",
            "icon": "./dist/assets/icons/mac/icon.icns"
        },
        "win": {
            "target": "dir",
            "icon": "./dist/assets/icons/win/icon.ico"
        },
        "linux": {
            "target": "dir",
            "icon": "./dist/assets/icons/png"
        }
    },
    "directories": {
        "output": "release",
        "app": "src"
    },
    "bin": {
        "electron": "./node_modules/.bin/electron"
    },
    "scripts": {
        "postinstall": "install-app-deps",
        "build": "gulp build",
        "prerelease": "gulp build --env=production",
        "release": "build",
        "start": "gulp start",
        "pretest": "gulp build-unit --env=test",
        "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
        "coverage": "npm test -- -R scripts/istanbul-reporter",
        "pree2e": "gulp build-e2e --env=test",
        "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register",
        "start-hot": "HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/background.js"
    },
    "dependencies": {
        "admin-lte": "^2.3.11",
        "angular": "^1.6.1",
        "angular-animate": "^1.6.1",
        "angular-aria": "^1.6.1",
        "angular-cookies": "^1.6.1",
        "angular-resource": "^1.6.1",
        "angular-sanitize": "^1.6.1",
        "angular-ui-ace": "^0.2.3",
        "angular-ui-bootstrap": "^2.4.0",
        "angular-ui-router": "next",
        "bluebird": "^3.4.7",
        "bootstrap": "^3.3.7",
        "brace": "^0.9.1",
        "chokidar": "^1.6.1",
        "core-js": "^2.4.1",
        "font-awesome": "^4.7.0",
        "fs-jetpack": "^0.10.2",
        "jquery": "2.2.3",
        "jquery-slimscroll": "^1.3.8",
        "jquery-ui": "^1.12.1",
        "lodash": "^4.17.4",
        "nodemailer-wellknown": "^0.2.1",
        "rx-angular": "^1.1.3",
        "source-map-support": "^0.4.2",
        "through2": "^2.0.3",
        "winston": "^2.3.1",
        "xml2js": "^0.4.17"
    },
    "devDependencies": {
        "angular-hot-reloader": "^0.0.2",
        "autoprefixer": "^6.6.1",
        "babel-core": "^6.21.0",
        "babel-loader": "^6.2.10",
        "babel-plugin-transform-class-properties": "^6.19.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-polyfill": "^6.20.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-register": "^6.22.0",
        "chai": "^3.5.0",
        "css-loader": "^0.26.1",
        "del": "^2.2.2",
        "electron": "beta",
        "electron-builder": "^8.6.0",
        "electron-connect": "^0.6.1",
        "electron-mocha": "^3.0.0",
        "express": "^4.14.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.9.0",
        "fs-extra": "^2.0.0",
        "gulp": "^3.9.0",
        "gulp-batch": "^1.0.5",
        "gulp-file": "^0.3.0",
        "gulp-inject": "^4.2.0",
        "gulp-less": "^3.0.3",
        "gulp-load-plugins": "^1.4.0",
        "gulp-ng-config": "^1.4.0",
        "gulp-plumber": "^1.1.0",
        "gulp-sort": "^2.0.0",
        "gulp-util": "^3.0.6",
        "gulp-watch": "^4.3.5",
        "gulp-zip": "^4.0.0",
        "html-webpack-harddisk-plugin": "^0.0.2",
        "html-webpack-plugin": "^2.26.0",
        "imports-loader": "^0.7.0",
        "isparta-instrumenter-loader": "^1.0.1",
        "isparta-loader": "^2.0.0",
        "istanbul": "^0.4.3",
        "istanbul-instrumenter-loader": "^1.2.0",
        "json-loader": "^0.5.4",
        "less": "^2.7.1",
        "less-loader": "^2.2.3",
        "minimist": "^1.2.0",
        "mocha": "^3.0.2",
        "ng-annotate-loader": "^0.2.0",
        "null-loader": "^0.1.1",
        "postcss-loader": "^1.2.1",
        "raw-loader": "^0.5.1",
        "rollup": "^0.36.3",
        "rollup-plugin-istanbul": "^1.1.0",
        "run-sequence": "^1.2.2",
        "spectron": "^3.3.0",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack-dev-middleware": "^1.9.0",
        "webpack-dev-server": "^1.16.2",
        "webpack-hot-middleware": "^2.15.0",
        "webpack-stream": "^3.2.0",
        "ws": "^1.1.1"
    },
    "optionalDependencies": {
        "windows-shortcuts": "^0.1.4"
    }
}
