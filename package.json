    "rollup-plugin-istanbul": "^1.1.0",
    "run-sequence": "^1.2.2",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0",
    "style-loader": "^0.13.1",
    "webpack-stream": "^3.2.0"
